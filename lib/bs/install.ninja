rescript = 1
rule cp
  command = cmd.exe /C copy /Y $i $out >NUL
rule touch
 command = cmd.exe /C type nul >>$out & copy $out+,, >NUL
o Test.cmi : cp ..\bs\src\components\Test.cmi
o Test.cmj : cp ..\bs\src\components\Test.cmj
o Test.cmt : cp ..\bs\src\components\Test.cmt
o Test.res : cp ..\..\src\components\Test.res
build install.stamp : touch Test.cmi Test.cmj 
